unit BisParamVisible;

interface

uses Classes,
     BisParam, BisParamInvisible;

type

  TBisParamVisible=class(TBisParamInvisible)
  protected
    function UseTempValue: Boolean; override;
  public
    constructor Create; override;
  end;

implementation

uses Variants, Sysutils,
     BisUtils;

{ TBisParamVisible }

constructor TBisParamVisible.Create;
begin
  inherited Create;
  Visible:=true;
end;


end.
