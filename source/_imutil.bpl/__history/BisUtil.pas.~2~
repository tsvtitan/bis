unit BisUtil;

interface

uses BisModules, BisIfaceModules;

procedure InitIfaceModule(AModule: TBisIfaceModule); stdcall;                 

exports
  InitIfaceModule;

implementation

uses BisUtilFm;

procedure InitIfaceModule(AModule: TBisIfaceModule); stdcall;
begin
  UtilIface:=AModule.Ifaces.AddClass(TBisUtilFormIface);
end;

end.
