VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = B305_D41.bpl
OBJFILES = ..\source\AbPeCol.obj ..\source\Abpedir.obj ..\source\Abpefn.obj \
  ..\source\Abpepass.obj ..\source\Abpever.obj ..\source\Abregvcl.obj B305_D41.obj
RESFILES = B305_D41.res
DEFFILE =
RESDEPEN = $(RESFILES) ..\source\AbPeCol.dfm ..\source\Abpever.dfm
LIBFILES =
IDLGENFILES =
IDLFILES =
LIBRARIES = dclocx40.lib inet40.lib \
  inetdb40.lib nmfast40.lib dss40.lib tee40.lib teedb40.lib teeui40.lib \
  qrpt40.lib vcldbx40.lib ibsmp40.lib bcbsmp40.lib vcldb40.lib vclmid40.lib \
  vcljpg40.lib
SPARELIBS = vcl40.lib vclx40.lib vcljpg40.lib vclmid40.lib vcldb40.lib bcbsmp40.lib \
  ibsmp40.lib vcldbx40.lib qrpt40.lib teeui40.lib teedb40.lib tee40.lib dss40.lib \
  nmfast40.lib inetdb40.lib inet40.lib dclocx40.lib
PACKAGES = vcl40.bpi Vclx40.bpi B305_r41.bpi
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;
PATHPAS = .;..\source
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
SYSDEFINES = NO_STRICT;USEPACKAGES
USERDEFINES =
# ---------------------------------------------------------------------------
CFLAG1 = -I..\source;$(BCB)\include;$(BCB)\include\vcl -O2 -w -Ve -Tkh30000 -a8 -k- -vi \
  -D$(SYSDEFINES);$(USERDEFINES) -c -b- -w-par -w-inl -Vx -tWM
CFLAG2 =
CFLAG3 =
IDLCFLAGS = -I..\source -I$(BCB)\include -I$(BCB)\include\vcl -src_suffixcpp
PFLAGS = -U..\source;$(BCB)\Projects\Lib;..\abbrevia3;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I..\source;$(BCB)\include;$(BCB)\include\vcl -$YD -$L- -$D- -v -M -JPHNE
RFLAGS = -i..\source;$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /i..\source /i$(BCB)\include /i$(BCB)\include\vcl /mx /w2 /zn /dUSEPACKAGES
LFLAGS = -L..\source;$(BCB)\Projects\Lib;..\abbrevia3;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -D"TurboPower Abbrevia 3.05 Design-time package - VCL41" -aa -Tpp -Gpd -x -Gn \
  -Gl -Gi -w
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj $(PACKAGES) sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
