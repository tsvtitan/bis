  end;  
end;

procedure UnLoadDLL;
begin
  if DLLHandle<>0 then
    FreeLibrary(DLLHandle);
  DLLHandle:=0;  
end;

function PRAPILoaded: Boolean;
begin
  Result:=DLLHandle<>0;
end;
  
initialization
  LoadDLL;

finalization
  UnLoadDLL;

end.
