unit BisDws;

interface

uses BisScriptModules, BisModules, BisCoreObjects,
     BisCoreIntf;

procedure InitScriptModule(AModule: TBisScriptModule); stdcall;

exports
  InitScriptModule;

implementation

uses BisScmDwsScript;

procedure InitScriptModule(AModule: TBisScriptModule); stdcall;
begin
  AModule.ScriptClass:=TBisScmDwsScript;
end;

end.
