unit BisDws;

interface

uses BisScriptModules, BisModules, BisCoreObjects,
     BisCoreIntf;

procedure InitScriptModule(AModule: TBisScriptModule); stdcall;

exports
  InitScriptModule;

implementation

uses BisDwsScript;

procedure InitScriptModule(AModule: TBisScriptModule); stdcall;
begin
  AModule.ScriptClass:=TBisDwsScript;
end;

end.
